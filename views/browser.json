{
	"Sources": {
		"Core": "{Core}",
		"Extensions": "{Extensions}",
		"Local": "{local}"
	},
	"Modules": {
		"Evo": {
			"Module": "Test",
			"Source": "Local",
			"Par": {
				"Root": true,
				"Children": "$Explorer"
			}
		},
		"Explorer": {
			"Module": "BrokerExplorer",
			"Source": "Local",
			"Par": {
				"Broker": "$LocalModules"
			}
		},
		"LocalModules": {
			"Module": "xGraph.WebProxy",
			"Source": "Core",
			"Par": {
				"Link": "$LocalModules"
			}
		}
	}
}