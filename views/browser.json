{
	"Sources": {
		"Core": "{Core}",
		"Extensions": "{Extensions}",
		"Local": "{local}"
	},
	"Modules": {
		"Evo": {
			"Module": "Test",
			"Source": "Local",
			"Par": {
				"Root": true,
				"Children": [
					"$Explorer",
					"$PreEditor"
				],
				"System": "$System"
			}
		},
		"Explorer": {
			"Module": "BrokerExplorer",
			"Source": "Local",
			"Par": {
				"Broker": "$thingy",
				"Editor": "$PreEditor"
			}
		},
		"PreEditor": {
			"Module": "Monaco",
			"Source": "Local",
			"Par": {
				"Text": "Default Text",
				"Broker": "$thingy"
			}
		},
		"thingy": {
			"Module": "xGraph.WebProxy",
			"Source": "Core",
			"Par": {
				"Link": "LocalModules"
			}
		},
		"System": {
			"Module": "xGraph.WebProxy",
			"Source": "Core",
			"Par": {
				"Link": "System"
			}
		}
	}
}