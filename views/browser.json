{
	"Sources": {
		"Local": "{local}",
		"Core": {
			"Host": "modulebroker.xgraphdev.com",
			"Port": 27000
		}
	},
	"Modules": {
		"Evo": {
			"Module": "Frame",
			"Source": "Local",
			"Par": {
				"Root": true,
				"Children": 
					{
						"View": "$Panel",
						"Children": [
							"$Explorer",
							"$Monaco"
						]
					}
				,
				"System": "$System"
			}
		},
		"Panel": {
			"Module": "PanelView",
			"Source": "Core",
			"Par": {
				"Ratio": 0.3
			}
		},
		"Explorer": {
			"Module": "BrokerExplorer",
			"Source": "Local",
			"Par": {
				"Editor": "$Monaco"
			}
		},
		"Monaco": {
			"Module": "Monaco",
			"Source": "Local",
			"Par": {
				"Text": "Default Text",
				"Broker": "$thingy"
			}
		},
		"System": {
			"Module": "WebSocketClientProxy",
			"Source": "Local",
			"Par": {
				"Host": "192.168.2.209",
				"Port": "52310"
			}
		}
	}
}