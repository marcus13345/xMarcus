{
	"Sources": {
		"Local": "{local}",
		"DevCore": "{devcore}",
		"Core": {
			"Host": "modulebroker.xgraphdev.com",
			"Port": 27000
		}
	},
	"Modules": {
		"Frame": {
			"Module": "Frame",
			"Source": "Local",
			"Par": {
				"Root": true,
				"Children": [
					{
						"View": "$Panel",
						"Children": [
							"$Explorer",
							"$Monaco"
						]
					}
				],
				"System": "$System"
			}
		},
		"SystemOfSystems": {
			"Module": "SystemOfSystems",
			"Source": "Local",
			"Par": {
				"Sources": {
					"Core": {
						"Host": "modulebroker.xgraphdev.com",
						"Port": 27002
					}
				},
				"Systems": {
					"Evo HTTP Server": "$System"
				}
			}
		},
		"Panel": {
			"Module": "PanelView",
			"Source": "DevCore",
			"Par": {
				"Ratio": 0.17,
				"Horizontal": true
			}
		},
		"Explorer": {
			"Module": "BrokerExplorer",
			"Source": "Local",
			"Par": {
				"Editor": "$Monaco",
				"SystemOfSystems": "$SystemOfSystems"
			}
		},
		"Monaco": {
			"Module": "Monaco",
			"Source": "Local",
			"Par": {
				"Text": "Default Text",
				"Broker": "$thingy"
			}
		},
		"System": {
			"Module": "WebSocketClientProxy",
			"Source": "Local",
			"Par": {
				"Host": "localhost",
				"Port": 52311
			}
		},
		"Deferred": [
			{
				"Module": "ModuleCache",
				"Source": "Local"
			},
			{
				"Module": "WebSocketClientProxy",
				"Source": "Local"
			}
		]
	}
}