{
	"Sources": {
		"Core": "{Core}",
		"Extensions": "{Extensions}",
		"Local": "{local}"
	},
	"Modules": {
		"Server": {
			"Module": "xGraph.WebViewer",
			"Source": "Core",
			"Par": {
				"ApexList": {
					"LocalModules": "$LocalModules",
					"System": "$TestSystem"
				},
				"Port": "{Port}",
				"Url": "",
				"HTML": "@file: browser.html",
				"": "@system: browser.json",
				"Static": "@directory: static"
			}
		},
		"LocalModules": {
			"Module": "RepositoryAdapter",
			"Source": "Local",
			"Par": {
				"BrokerPath": "./modules"
			}
		},
		"TestSystem": {
			"Module": "xGraph.Proxy",
			"Source": "Core",
			"Par": {
				"Role": "client",
				"Host": "127.0.0.1",
				"Port": "{ConnectTo}",
				"Poll": true,
				"Encrypt": false
			}
		},
		"TestSystemServer": {
			"Module": "xGraph.Proxy",
			"Source": "Core",
			"Par": {
				"Role": "server",
				"Host": "127.0.0.1",
				"Port": "{SysPort}",
				"Link": "$Self",
				"Encrypt": false
			}
		},
		"Self": {
			"Module": "System",
			"Source": "Local",
			"Par": {
				"Name": "{SystemName}"
			}
		}
	}
}