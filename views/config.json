{
	"Sources": {
		"Core": "{Core}",
		"Local": "{local}"
	},
	"Modules": {
		"Server": {
			"Module": "xGraph.WebViewer",
			"Source": "Core",
			"Par": {
				"ApexList": {
					"LocalModules": "$LocalModules",
					"System": "$TestSystem"
				},
				"Port": "{Port}",
				"Url": "",
				"HTML": "@file: browser.html",
				"": "@system: browser.json",
				"Static": "@directory: static"
			}
		},
		"LocalModules": {
			"Module": "xGraph.Proxy",
			"Source": "Core",
			"Par": {
				"Role": "client",
				"Host": "127.0.0.1",
				"Port": "27000",
				"Poll": true
			}
		},
		"TestSystem": {
			"Module": "xGraph.Proxy",
			"Source": "Core",
			"Par": {
				"Role": "client",
				"Host": "127.0.0.1",
				"Port": "2800",
				"Poll": true
			}
		}
	}
}