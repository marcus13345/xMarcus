{
	"Sources": {
		"Core": {
			"Host": "modulebroker.xgraphdev.com",
			"Port": 27000
		},
		"DevCore": "{devcore}",
		"Local": "{local}"
	},
	"Modules": {
		"Server": {
			"Module": "WebViewer",
			"Source": "DevCore",
			"Par": {
				"ApexList": {
				},
				"Port": 80,
				"Url": "",
				"HTML": "@file: browser.html",
				"": "@system: browser.json",
				"Static": "@directory: static"
			}
		},
		"Self": {
			"Module": "System",
			"Source": "Local",
			"Par": {
				"Name": "HTTP Webserver"
			}
		},
		"SelfProxy": {
			"Module": "WebsocketServerProxy",
			"Source": "Core",
			"Par": {
				"Link": "$Self",
				"Port": 52311
			}
		}
	}
}