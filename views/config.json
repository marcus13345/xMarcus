{
	"Sources": {
		"Core": "{Core}",
		"Extensions": "{Extensions}",
		"Local": "{local}"
	},
	"Modules": {
		"Server": {
			"Module": "xGraph.WebViewer",
			"Source": "Core",
			"Par": {
				"ApexList": {
					"LocalModules": "$LocalModules"
				},
				"Port": 8080,
				"Url": "",
				"HTML": "@file: browser.html",
				"": "@system: browser.json",
				"Static": "@directory: static"
			}
		},
		"LocalModules": {
			"Module": "RepositoryAdapter",
			"Source": "Local",
			"Par": {
				"BrokerPath": "./modules"
			}
		}
	}
}